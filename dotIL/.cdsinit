;dilHome = strcat( skillHome "/dotIL" )
dilHome = strcat( "/home/pakhomov.s/skill/dotIL" )
load( strcat( dilHome "/.il" ) )

let( (itemInit menuDIL)
	ciwMenuInit()
	itemInit = hiCreateMenuItem(
			?name 'itemInit
			?itemText "Initializtion"
			?callback "DILInit() DILLoadSKILL()"
	)
	itemLog = hiCreateMenuItem(
			?name 'itemILog
			?itemText "About"
			?callback "DILAbout()"
	)
	menuDIL = hiCreatePulldownMenu( 
			'menuDIL
			".il"
			list(
				itemInit
				itemLog
			)
	)
	hiInsertBannerMenu( window(1) menuDIL hiGetNumMenus(window(1)) )
	printf("[INFO] Menu \".il\" has been added to CIW\n")
);let
